# Environment Configuration Example
# Copy this file to .env.local and update with your actual values

# API Configuration
VITE_API_BASE_URL=https://api.financial-platform.com
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_DOMAIN=your-auth-domain.com
VITE_AUTH_CLIENT_ID=your-client-id

# Feature Flags
VITE_ENABLE_REAL_TIME_DATA=false
VITE_ENABLE_ADVANCED_ANALYTICS=false
VITE_ENABLE_DARK_MODE=true

# Development
VITE_LOG_LEVEL=debug
VITE_MOCK_API=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_SENTRY_DSN=https://your-sentry-dsn.com

# Market Data
VITE_MARKET_DATA_WS_URL=wss://marketdata.financial-platform.com/ws
VITE_MARKET_DATA_API_KEY=your-market-data-api-key